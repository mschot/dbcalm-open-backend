{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: FASTAPI - custom",
            "type": "debugpy",
            "request": "launch",
            "program": "${cwd}/dbcalm-api.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONDONTWRITEBYTECODE": "1",
                "DBCALM_DEV_MODE": "1"
            }
        },
        {
            "name": "Python Debugger: cmd-server",
            "type": "debugpy",
            "request": "launch",
            "program": "${cwd}/dbcalm-cmd-server.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONDONTWRITEBYTECODE": "1",
                "DBCALM_DEV_MODE": "1"
            }
        }
    ]
}